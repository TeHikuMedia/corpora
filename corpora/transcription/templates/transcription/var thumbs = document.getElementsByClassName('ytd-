var thumbs = document.getElementsByClassName('ytd-thumbnail-overlay-time-status-renderer');
var m=[];
var s=[];
var dur = 0;
for (let video of thumbs){
	var text = video.innerHTML
	parts = text.split(":");
	m.push(parseInt(parts[0]));
	s.push(parseInt(parts[1]));
	dur = dur + parseInt(parts[0])*60+parseInt(parts[1])
}
console.log(dur)
console.log(thumbs.length)
console.log(dur/thumbs.length)




var v = document.querySelectorAll('#metadata-line span.ytd-grid-video-renderer:first-child')
var views = 0.0
var count = 1.0
for (let i in v){
	var elm = v[i]
	try{
	  var text = elm.innerHTML;
	  var parts = text.split(" views");
	  try{
	  	if (parts[0].search('K')>0){
	  		views = views + parseFloat(parts[0].split('K')[0])*1000
	  	} else if (parts[0].search('M')>0){
		  	views = views + parseFloat(parts[0].split('K')[0])*1000*1000
	  	}
	  	else{
	  		views = views + parseFloat(parts[0].split('K')[0])
	  	}
	  	count = count + 1
	  } catch{

	  }
	} catch{
	}
}
console.log(views)
console.log(count)
console.log(views/count)